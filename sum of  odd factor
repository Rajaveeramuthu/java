class OddFactor{
    public static void main(String[] args){
        int n=55;
        int sum=0;
        for(int i=1;i<=n;i++){
            if(n%i==0 && i%2!=0){
                // System.out.print(i+" ");
                sum=sum+i;
            }
        }
        System.out.println();
        System.out.print("sum of the number"+sum);
    }
}
